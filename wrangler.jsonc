{
    "$schema": "node_modules/wrangler/config-schema.json",
    "name": "blog",
    "compatibility_date": "2026-01-01",
    "assets": {
        "directory": "./dist"
    },
    "kv_namespaces": [
        {
            "binding": "SESSION",
            "id": "SESSION_NAMESPACE_ID",
            "preview_id": "SESSION_PREVIEW_ID"
        }
    ],
    "observability": {
        "enabled": true
    }
}